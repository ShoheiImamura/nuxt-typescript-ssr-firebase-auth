{
  "name": "nuxt-typescript-ssr-firebase-auth",
  "version": "1.0.0",
  "description": "nuxt-typescript-firebase-auth-ssr",
  "author": "Resul Avan",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "eslint --fix --ext .ts,.js,.vue .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {}
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/tests/jest-mocks.ts"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.{ts,vue}",
      "<rootDir>/pages/**/*.vue"
    ]
  },
